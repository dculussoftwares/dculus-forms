{
  "name": "dculus-forms",
  "version": "1.0.0",
  "description": "A monorepo for Dculus Forms with Express.js + GraphQL backend and React + Material UI frontend",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --parallel --filter backend --filter form-app --filter form-viewer --filter admin-app run dev",
    "build": "pnpm run --recursive build",
    "clean": "pnpm run --recursive clean",
    "lint": "pnpm run --recursive lint",
    "test": "pnpm run --recursive test",
    "type-check": "pnpm run --recursive type-check",
    "backend:dev": "pnpm --filter backend dev",
    "backend:build": "pnpm --filter backend build",
    "backend:start": "pnpm --filter backend start",
    "form-app:dev": "pnpm --filter form-app dev",
    "form-app:build": "pnpm --filter form-app build",
    "form-app:preview": "pnpm --filter form-app preview",
    "form-viewer:dev": "pnpm --filter form-viewer dev",
    "form-viewer:build": "pnpm --filter form-viewer build",
    "form-viewer:preview": "pnpm --filter form-viewer preview",
    "admin-app:dev": "pnpm --filter admin-app dev",
    "admin-app:build": "pnpm --filter admin-app build",
    "admin-app:preview": "pnpm --filter admin-app preview",
    "db:generate": "pnpm --filter backend db:generate",
    "db:push": "pnpm --filter backend db:push",
    "db:studio": "pnpm --filter backend db:studio",
    "db:seed": "pnpm --filter backend db:seed",
    "db:migrate-layout": "pnpm --filter backend db:migrate-layout",
    "db:migrate-bg-images": "pnpm --filter backend db:migrate-bg-images",
    "db:setup": "./scripts/setup-db.sh",
    "admin:setup": "pnpm --filter backend admin:setup",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "ui:storybook": "pnpm --filter @dculus/ui storybook",
    "ui:build-storybook": "pnpm --filter @dculus/ui build-storybook",
    "test:integration": "TS_NODE_PROJECT=test/integration/tsconfig.json cucumber-js 'test/integration/features/*.feature' --require-module ts-node/register --require test/integration/support/world.ts --require test/integration/support/hooks.ts --require test/integration/step-definitions/common.steps.ts --require 'test/integration/step-definitions/*.steps.ts'",
    "test:integration:watch": "pnpm test:integration --watch",
    "test:integration:phase1": "TS_NODE_PROJECT=test/integration/tsconfig.json cucumber-js 'test/integration/features/form-lifecycle.feature' 'test/integration/features/form-responses.feature' --require-module ts-node/register --require test/integration/support/world.ts --require test/integration/support/hooks.ts --require test/integration/step-definitions/common.steps.ts --require 'test/integration/step-definitions/*.steps.ts'",
    "test:integration:forms": "TS_NODE_PROJECT=test/integration/tsconfig.json cucumber-js 'test/integration/features/form-lifecycle.feature' --require-module ts-node/register --require test/integration/support/world.ts --require test/integration/support/hooks.ts --require test/integration/step-definitions/common.steps.ts --require 'test/integration/step-definitions/*.steps.ts'",
    "test:integration:responses": "TS_NODE_PROJECT=test/integration/tsconfig.json cucumber-js 'test/integration/features/form-responses.feature' --require-module ts-node/register --require test/integration/support/world.ts --require test/integration/support/hooks.ts --require test/integration/step-definitions/common.steps.ts --require 'test/integration/step-definitions/*.steps.ts'",
    "test:integration:admin": "TS_NODE_PROJECT=test/integration/tsconfig.json cucumber-js 'test/integration/features/admin-signup.feature' --require-module ts-node/register --require test/integration/support/world.ts --require test/integration/support/hooks.ts --require test/integration/step-definitions/common.steps.ts --require 'test/integration/step-definitions/*.steps.ts'",
    "test:integration:auth-graphql": "./test-auth-graphql.sh",
    "test:e2e": "./test/e2e/scripts/run-e2e-tests.sh",
    "test:e2e:dev": "TS_NODE_PROJECT=test/e2e/tsconfig.json cucumber-js 'test/e2e/features/*.feature' --require-module ts-node/register --require test/e2e/support/world.ts --require test/e2e/support/hooks.ts --require 'test/e2e/step-definitions/*.steps.ts'",
    "test:e2e:headed": "TS_NODE_PROJECT=test/e2e/tsconfig.json PLAYWRIGHT_HEADLESS=false cucumber-js 'test/e2e/features/*.feature' --require-module ts-node/register --require test/e2e/support/world.ts --require test/e2e/support/hooks.ts --require 'test/e2e/step-definitions/*.steps.ts'",
    "test:e2e:form-creation": "TS_NODE_PROJECT=test/e2e/tsconfig.json cucumber-js 'test/e2e/features/*.feature' --require-module ts-node/register --require test/e2e/support/world.ts --require test/e2e/support/hooks.ts --require 'test/e2e/step-definitions/*.steps.ts' --tags '@PageInteraction'",
    "test:e2e:form-creation:headed": "TS_NODE_PROJECT=test/e2e/tsconfig.json PLAYWRIGHT_HEADLESS=false cucumber-js 'test/e2e/features/*.feature' --require-module ts-node/register --require test/e2e/support/world.ts --require test/e2e/support/hooks.ts --require 'test/e2e/step-definitions/*.steps.ts' --tags '@PageInteraction'"
  },
  "devDependencies": {
    "@cucumber/cucumber": "12.2.0",
    "@playwright/test": "^1.52.0",
    "@types/country-list": "2.1.4",
    "@types/form-data": "2.5.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "6.0.3",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "axios": "1.11.0",
    "dotenv": "17.2.1",
    "eslint": "^8.55.0",
    "form-data": "4.0.4",
    "jest": "^29.7.0",
    "playwright": "^1.52.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.2",
    "typescript": "^5.3.0",
    "ws": "8.18.3",
    "yjs": "13.6.27"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "@types/react": "^18.3.0",
      "@types/react-dom": "^18.3.0",
      "react": "^18.3.0",
      "react-dom": "^18.3.0"
    }
  },
  "dependencies": {
    "@maxmind/geoip2-node": "6.1.0",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-slot": "1.2.3",
    "@tanstack/react-table": "8.21.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "country-list": "^2.2.0",
    "graphql-request": "7.2.0",
    "lucide-react": "0.534.0",
    "nanoid": "5.1.5",
    "tailwind-merge": "3.3.1",
    "ua-parser-js": "^1.0.37"
  }
}