{
  "name": "@dculus/utils",
  "version": "0.1.0",
  "description": "Shared utilities for Dculus Forms",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json && find dist-cjs -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.cjs\"' _ {} \\; && node fix-cjs-imports.cjs"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "nanoid": "5.1.5"
  }
}